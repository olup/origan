# Garage S3-compatible storage
GARAGE_ENDPOINT=https://s3.platform.origan.dev
GARAGE_ACCESS_KEY=your-access-key
GARAGE_SECRET_KEY=your-secret-key

# Docker Registry (optional, for pushing images)
REGISTRY_USERNAME=your-registry-username
REGISTRY_PASSWORD=your-registry-password

# GitHub App Configuration (for Control API)
GITHUB_APP_ID=your-github-app-id
GITHUB_APP_PRIVATE_KEY=your-github-app-private-key
GITHUB_WEBHOOK_SECRET=your-webhook-secret

# Image tags (optional, defaults to timestamp)
IMAGE_TAG=latest